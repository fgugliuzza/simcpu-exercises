DIV: LDWI R0 0000
DIV_LOOP: MV R1 R3
          SUB R2 R1
          JMPN DIV_END
          INC R0
          JMP DIV_LOOP
DIV_END: RET

START: LDWI R8 0f000 ; Fix per un bug dell'assembler
       SPWR R8 ; Inizializzo lo stack pointer
       LDWI R1 0069
       LDWI R2 000a
       CALL DIV
       HLT
